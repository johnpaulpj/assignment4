{

  "manifest_version": 2,
  "name": "Moz Extension Ass",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icon/icon_one.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["sivambi.js"]
    }
  ]
}